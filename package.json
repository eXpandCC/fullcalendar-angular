{
  "private": true,
  "name": "fullcalendar-angular-workspace",
  "version": "5.9.0",
  "scripts": {
    "ng": "node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "build": "npm run build:meta && ng build fullcalendar --prod",
    "build:meta": "mkdir -p dist/fullcalendar && cp LICENSE.* README.* dist/fullcalendar",
    "build:demo": "ng build fullcalendar-demo --configuration='production'",
    "test": "ng test fullcalendar --watch=false --browsers ChromeHeadless",
    "test:watch": "ng test fullcalendar --watch=true",
    "test:demo": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "clean": "./scripts/clean.sh",
    "ci": "./scripts/ci.sh",
    "sync-version-fields": "./scripts/sync-version-fields.js",
    "version": "npm run sync-version-fields && git add projects/fullcalendar/package.json"
  },
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@fullcalendar/core": "~5.9.0",
    "@fullcalendar/daygrid": "~5.9.0",
    "@fullcalendar/interaction": "~5.9.0",
    "@fullcalendar/timegrid": "~5.9.0",
    "core-js": "^3.6.2",
    "fast-deep-equal": "^3.1.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.1",
    "@angular/language-service": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.26.0",
    "fullcalendar-scheduler": "~5.9.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.5.4",
    "tslint": "~6.1.0",
    "typescript": "4.2.4",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest"
  }
}
